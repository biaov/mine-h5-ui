{
  "name": "mine-h5-ui",
  "version": "1.6.2-beta.1",
  "private": false,
  "author": "biaov<biaov@qq.com>",
  "main": "lib/mine-h5-ui.umd.min.js",
  "style": "lib/theme-default/index.css",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "npm run build:docs && npm run build:lib && npm run build:theme",
    "build:docs": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name mine-h5-ui --dest lib packages/index.js",
    "build:theme": "gulp build -f build/gulpfile.prod.js",
    "build:theme-rem": "gulp build -f build/gulpfile.rem.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "pre-publish": "node build/pre-publish",
    "pre-publish:github": "cross-env NODE_PRE=github node build/pre-publish",
    "prettier": "prettier --write '**/*.{ts,js,less,md,json}'"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/packages/$1"
    }
  },
  "keywords": [
    "MINE-H5-UI",
    "Vue",
    "VueJs",
    "UI组件"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biaov/MINE-H5-UI.git"
  },
  "homepage": "https://mineh5ui.biaov.cn/",
  "bugs": {
    "url": "https://github.com/biaov/MINE-H5-UI/issues"
  },
  "publishConfig": {},
  "license": "ISC",
  "contributors": [
    {
      "name": "biaov",
      "email": "biaov@qq.com"
    }
  ],
  "dependencies": {
    "core-js": "^3.6.4",
    "html2canvas": "^1.3.2",
    "mockjs": "^1.1.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "del": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "husky": "^7.0.4",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "markdown-it-container": "^2.0.0",
    "postcss-pxtorem": "^5.1.1",
    "style-resources-loader": "^1.3.3",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.11"
  }
}
