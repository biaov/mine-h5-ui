{
  "name": "mine-h5-ui",
  "version": "2.3.8",
  "private": false,
  "author": {
    "name": "biaov",
    "email": "biaov@qq.com",
    "url": "https://biaov.cn/"
  },
  "main": "es/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "style": "lib/theme-default/index.css",
  "description": "一款轻量级、模块化基于 VUE3 的 H5 前端 UI 组件库 👍",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite -m development",
    "build": "npm run build:docs && npm run build:theme && npm run build:package",
    "build:docs": "vite build",
    "build:theme": "gulp build -f build/gulpfile.prod.ts",
    "build:theme:rem": "gulp build -f build/gulpfile.rem.ts",
    "build:package": "cross-env NODE_OPTIONS=--max_old_space_size=8192 rollup -c build/rollup.config.ts",
    "preview": "vite preview",
    "lint": "npm run lint:examples && npm run lint:mobile && npm run lint:packages && npm run lint:tests",
    "lint:examples": "eslint --fix ./examples --ext .vue,.ts",
    "lint:mobile": "eslint --fix ./mobile --ext .vue,.ts",
    "lint:packages": "eslint --fix ./packages --ext .vue,.ts",
    "lint:tests": "eslint --fix ./tests --ext .ts",
    "test:unit": "jest -c=jest.config.js --detectOpenHandles",
    "test:unit:single": "jest tests/unit/button.spec.ts -c=jest.config.js",
    "prepare": "husky install",
    "pre-publish": "node build/pre-publish.ts && start cmd /k cd ./dist/packages",
    "pre-publish:github": "cross-env NODE_PRE=github node build/pre-publish.ts && start cmd /k cd ./dist/packages",
    "prettier": "prettier --write '**/*.{ts,js,less,md,json,vue}'",
    "ncu": "ncu --configFileName .ncurc.json && npm i"
  },
  "types": "es/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "MINE-H5-UI",
    "TypeScript",
    "Vue",
    "Vue3",
    "VueJs",
    "UI组件"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biaov/MINE-H5-UI.git"
  },
  "homepage": "https://mineh5ui.biaov.cn/v2",
  "bugs": {
    "url": "https://github.com/biaov/MINE-H5-UI/issues"
  },
  "publishConfig": {},
  "license": "ISC",
  "contributors": [
    {
      "name": "biaov",
      "email": "biaov@qq.com"
    }
  ],
  "dependencies": {
    "clipboard": "^2.0.11",
    "html2canvas": "^1.4.1",
    "mockjs": "^1.1.0",
    "qrcode": "^1.5.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/jest": "^29.2.3",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.7.23",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.3",
    "@vue/vue3-jest": "^29.2.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^5.0.0",
    "gulp-postcss": "^9.0.1",
    "highlight.js": "^11.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "less": "^4.1.3",
    "markdown-it-container": "^3.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "vite": "^3.2.4",
    "vite-plugin-md": "^0.20.4",
    "vite-plugin-pwa": "^0.13.3",
    "vue-tsc": "^1.0.9"
  }
}
