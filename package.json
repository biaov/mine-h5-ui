{
  "name": "mine-h5-ui",
  "version": "2.14.2",
  "private": false,
  "author": {
    "name": "biaov",
    "email": "biaov@qq.com",
    "url": "https://biaov.cn/"
  },
  "main": "es/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "style": "styles/index.css",
  "description": "一款轻量级、模块化基于 Vue3.x 的 H5 前端 UI 组件库 👍",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite -m development",
    "build": "npm run build:docs && npm run build:theme && npm run build:theme:rem && npm run build:package",
    "build:docs": "vite build",
    "build:theme": "vite build --config scripts/theme.vite.ts",
    "build:theme:rem": "set rem=1 && vite build --config scripts/theme.vite.ts",
    "build:package": "vite build --config scripts/package.vite.ts",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:single": "vitest dev tests/unit/calendar.spec.ts",
    "test:ui": "vite-node ./tests/ui/index.ts",
    "prepare": "husky",
    "pre-publish": "node scripts/pre-publish.js",
    "pre-publish:github": "set NODE_PRE=github && node scripts/pre-publish.js && start cmd /k cd ./dist/packages",
    "lint": "eslint --fix ./**",
    "prettier": "prettier --write '**/*.{ts,js,less,md,json,vue,cjs}'",
    "tag": "node scripts/tag.js"
  },
  "types": "es/index.d.ts",
  "engines": {
    "node": ">=20.12"
  },
  "keywords": [
    "组件库",
    "typescript",
    "vue",
    "ui组件",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biaov/mine-h5-ui.git"
  },
  "homepage": "https://mineh5ui.biaov.cn/v2",
  "bugs": {
    "url": "https://github.com/biaov/mine-h5-ui/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "engine-strict": true,
    "provenance": true
  },
  "license": "ISC",
  "contributors": [
    {
      "name": "biaov",
      "email": "biaov@qq.com"
    }
  ],
  "dependencies": {
    "clipboard": "^2.0.11",
    "html2canvas": "^1.4.1",
    "mockjs": "^1.1.0",
    "qrcode": "^1.5.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@tailwindcss/vite": "^4.0.17",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.13.14",
    "@types/postcss-pxtorem": "^6.1.0",
    "@types/qrcode": "^1.5.5",
    "@types/selenium-webdriver": "^4.1.28",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "less": "^4.2.2",
    "markdown-it-container": "^4.0.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.5.3",
    "selenium-webdriver": "^4.30.0",
    "shiki": "^3.2.1",
    "tailwindcss": "^4.0.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "unplugin-vue-markdown": "^28.3.1",
    "vi-canvas-mock": "^1.0.0",
    "vite": "^6.2.3",
    "vite-node": "^3.0.9",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.21.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.2.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.37.0"
  }
}