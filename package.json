{
  "name": "mine-h5-ui",
  "version": "2.1.1",
  "private": false,
  "author": {
    "name": "biaov",
    "email": "biaov@qq.com"
  },
  "main": "lib/index.esm.js",
  "style": "lib/theme-default/index.css",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite -m development",
    "dev:test": "vite -m test",
    "dev:prod": "vite -m production",
    "build": "npm run build:theme && npm run build:package && npm run build:package:dts",
    "build:docs": "vite build",
    "build:theme": "gulp build -f build/gulpfile.prod.js",
    "build:theme:rem": "gulp build -f build/gulpfile.rem.js",
    "build:package": "rollup -c build/rollup.config.js",
    "build:package:dts": "rollup -c build/rollup.config.dts.js",
    "preview": "vite preview",
    "lint": "npm run lint:examples && npm run lint:mobile && npm run lint:packages && npm run lint:tests",
    "lint:examples": "eslint --fix ./examples --ext .vue,.ts",
    "lint:mobile": "eslint --fix ./mobile --ext .vue,.ts",
    "lint:packages": "eslint --fix ./packages --ext .vue,.ts",
    "lint:tests": "eslint --fix ./tests --ext .vue,.ts",
    "test:unit": "jest -c=jest.config.js --detectOpenHandles",
    "test:unit:single": "jest tests/unit/screenshot.spec.ts -c=jest.config.js --detectOpenHandles",
    "prepare": "husky install"
  },
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "MINE-H5-UI",
    "Vue",
    "Vue3",
    "VueJs",
    "UI组件"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/biaov/MINE-H5-UI.git"
  },
  "homepage": "https://mineh5ui.biaov.cn/v2",
  "bugs": {
    "url": "https://github.com/biaov/MINE-H5-UI/issues"
  },
  "license": "ISC",
  "contributors": [
    {
      "name": "biaov",
      "email": "biaov@qq.com"
    }
  ],
  "dependencies": {
    "clipboard": "^2.0.8",
    "html2canvas": "^1.3.2",
    "mockjs": "^1.1.0",
    "qrcode": "^1.4.4",
    "vue": "^3.0.11",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^15.0.2",
    "@types/qrcode": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.6",
    "autoprefixer": "^10.2.6",
    "del": "^6.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "less": "^3.13.1",
    "markdown-it-container": "^3.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.50.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vite-plugin-vuedoc": "^3.1.3",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.0.24"
  }
}
